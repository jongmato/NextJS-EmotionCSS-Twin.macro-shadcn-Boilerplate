{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "useTabs": false,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}
